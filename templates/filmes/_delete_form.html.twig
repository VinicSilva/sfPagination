<form method="post" action="{{ path('filmes_delete', {'id': filme.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ filme.id) }}">
    <button class="btn">Delete</button>
</form>